<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:vm="clr-namespace:Neat.ViewModels"
                    xmlns:v="clr-namespace:Neat.Views"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DataTemplate DataType="{x:Type vm:HomePageViewModel}">
        <v:HomePageView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PackManagerPageViewModel}">
        <v:PackManagerView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:NotificationViewModel}">
        <v:NotificationView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PopupViewModel}">
        <v:PopupView/>
    </DataTemplate>
</ResourceDictionary>